{
  "name": "fso",
  "version": "1.0.0",
  "description": "Backend of FullStackOpen's course (parts 3 and 4)",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV='production' node server.js",
    "dev": "cross-env NODE_ENV='development' npx nodemon server.js",
    "debug": "cross-env NODE_ENV='development' node --inspect server.js",
    "test": "cross-env NODE_ENV='test' jest --verbose --runInBand --forceExit",
    "clean": "rm -rf dist static index.html",
    "build:ui": "npm run clean && cd ../esb-r && npm run build && npm run dist",
    "deploy": "git push heroku HEAD:master",
    "lint": "npx eslint ./*.js",
    "pretty": "npx prettier --write ./*.js",
    "check": "npm run lint && npm run pretty"
  },
  "author": "dj-mc",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "mongoose": "^6.6.3",
    "morgan": "^1.10.0",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "eslint": "^8.24.0",
    "jest": "^29.1.2",
    "supertest": "^6.2.4"
  }
}
