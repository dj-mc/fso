{
  "name": "fso",
  "version": "1.0.0",
  "description": "Backend of FullStackOpen's course (parts 3 and 4)",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV='production' node server.js",
    "dev": "NODE_ENV='development' npx nodemon server.js",
    "debug": "NODE_ENV='development' node --inspect server.js",
    "test": "NODE_ENV='test' jest --verbose --runInBand --forceExit",
    "clean": "rm -rf dist static index.html",
    "build:ui": "npm run clean && cd ../esb-r && npm run build && npm run dist",
    "deploy": "git push heroku HEAD:master",
    "pretty": "npx prettier --write .",
    "check": "prettier --check './**/*.{js,ts,jsx,tsx,json,html,css,md}'",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx ."
  },
  "author": "dj-mc",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "mongoose": "^6.6.3",
    "morgan": "^1.10.0",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.1.2",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4"
  }
}
